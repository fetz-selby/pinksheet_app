<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.form-style{cursor:pointer; padding:10px;}
		.more-form-style{margin-top: -130px; }
		.upload-style{z-index: 4;}
	</ui:style>
	<g:HTMLPanel ui:field="mainPanel">
		<g:FormPanel styleName="form col-sm-12 col-xs-12 col-md-12 col-lg-12"
			ui:field="formPanel">
			<g:FileUpload styleName="col-sm-12 col-xs-12 col-md-12 col-lg-12 filestyle {style.form-style} {style.upload-style}"
				ui:field="upload"></g:FileUpload>
		</g:FormPanel>
	</g:HTMLPanel>
</ui:UiBinder> 
