<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel styleName="panel-body">

		<g:Button styleName="btn btn-sm btn-primary pull-right"
			ui:field="addBtn">+ Add</g:Button>
		<div class="clearfix"></div>

		<div class="form-group">
			<label>Role</label>
			<g:ListBox styleName="form-control" ui:field="typeBox"></g:ListBox>
		</div>

		<g:Button styleName="btn btn-sm btn-default pull-right"
			ui:field="submitBtn">Submit</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 