<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="panel-body">
		<div class="form-group">
			<label>Year</label>
			<g:ListBox styleName="form-control" ui:field="yearBox"></g:ListBox>
		</div>
	
		<div class="form-group">
			<label>Regions</label>
			<g:ListBox styleName="form-control" ui:field="regionBox"></g:ListBox>
		</div>
		
		<div class="form-group">
			<label>Constituency</label>
			<g:ListBox styleName="form-control" ui:field="constituencyBox"></g:ListBox>
		</div>
		
		<div class="form-group">
			<label>Polling Station</label>
			<g:ListBox styleName="form-control" ui:field="pollingBox"></g:ListBox>
		</div>
		
		<div class="form-group">
			<label>Type</label>
			<g:ListBox styleName="form-control" ui:field="typeBox"></g:ListBox>
		</div>
		
		<g:Button styleName="btn btn-sm btn-default pull-right" ui:field="submitBtn">Submit</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 